{
    "name": "fly-apps/dockerfile-laravel",
    "description": "Dockerfile generator and Fly.io helper.",
    "keywords": ["fly.io", "docker", "dockerfile"],
    "homepage": "https://fly.io/docs/laravel",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Fidao",
            "email": "fideloper@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22",
        "illuminate/view": "^10.0",
        "laravel-zero/framework": "^10.2",
        "nunomaduro/termwind": "^1.15.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/dockerfile-laravel"]
}
